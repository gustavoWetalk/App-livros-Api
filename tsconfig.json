{
  "compilerOptions": {
    "target": "ES2017",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true,
     "outDir": "./dist",
     "types": ["jest", "node"],
     "moduleResolution": "node",
  },
  "include": ["src/**/*", "tests/**/*"],
  "exclude": ["node_modules"]
}
